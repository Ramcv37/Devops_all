pipeline {
    agent any
    
    stages {
        stage('Clean Workspace') {
         steps {
           deleteDir()
          }
        }
        stage ('Checkout') {
            steps {
              echo 'this is git'
              sh "git init"
              sh "git pull origin master"
            }
        }
         stage ('build') {
            steps {
              echo 'this is build' 
              sh "ls"
              sh "git log --oneline"
            }
        }
         stage ('test') {
            steps {
               echo 'this is test' 
            }
        }
    }
}
