pipeline {
    agent any
    
    stages {
        stage ('Checkout') {
            steps {
              echo 'this is git'
              sh "git init"
              sh "git pull https://github.com/Ramcv37/Devops_all.git"
              sh "git config pull.rebase true"
            }
        }
         stage ('build') {
            steps {
              echo 'this is build' 
              sh "ls"
              sh "git log --oneline"
            }
        }
         stage ('test') {
            steps {
               echo 'this is test' 
            }
        }
    }
}
