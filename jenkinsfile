pipeline {
    agent any
    
    stages {
        stage('Clean Workspace') {
         steps {
           deleteDir()
          }
        }
        stage ('Checkout') {
            steps {
              echo 'this is git'
              dir('your_local_directory') {
            // 'your_local_directory' is the name of the local directory where you want to perform the git operations
            sh "git init"
            sh "git remote add origin https://github.com/Ramcv37/Devops_all.git"
            sh "git pull origin master"
            }
        }
       }
         stage ('build') {
            steps {
              echo 'this is build' 
              sh "ls"
              sh "git log --oneline"
            }
        }
         stage ('test') {
            steps {
               echo 'this is test' 
            }
        }
    }
}
